<?xml version="1.0"?>
<robot name="humanoid">
  <link name="base_link"/>

  <link name="torso">
    <visual>
      <geometry>
        <box size="0.4 0.2 0.6"/>
      </geometry>
    </visual>
  </link>

  <joint name="base_to_torso" type="fixed">
    <parent link="base_link"/>
    <child link="torso"/>
  </joint>

  <link name="head">
    <visual>
      <geometry>
        <sphere radius="0.15"/>
      </geometry>
    </visual>
  </link>

  <joint name="torso_to_head" type="revolute">
    <parent link="torso"/>
    <child link="head"/>
    <origin xyz="0 0 0.45"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
  </joint>

  <link name="left_arm">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.5"/>
      </geometry>
    </visual>
  </link>

  <joint name="torso_to_left_arm" type="revolute">
    <parent link="torso"/>
    <child link="left_arm"/>
    <origin xyz="0 0.15 0.2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="1"/>
  </joint>

  <link name="right_arm">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.5"/>
      </geometry>
    </visual>
  </link>

  <joint name="torso_to_right_arm" type="revolute">
    <parent link="torso"/>
    <child link="right_arm"/>
    <origin xyz="0 -0.15 0.2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="1"/>
  </joint>

  <link name="left_leg">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.7"/>
      </geometry>
    </visual>
  </link>

  <joint name="torso_to_left_leg" type="revolute">
    <parent link="torso"/>
    <child link="left_leg"/>
    <origin xyz="0 0.1 -0.3"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
  </joint>

  <link name="right_leg">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.7"/>
      </geometry>
    </visual>
  </link>

  <joint name="torso_to_right_leg" type="revolute">
    <parent link="torso"/>
    <child link="right_leg"/>
    <origin xyz="0 -0.1 -0.3"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
  </joint>

</robot>
